#footer {
  margin-top: 80px;
  padding: 40px 20px;
  background-color: #1B998A;
  color: #fff;
  font-size: 0.8rem;
}

#footer a {
  color: #fff;
  text-decoration: none;
}

#footer a:hover {
  text-decoration: underline;
}

#footer ul {
  margin-bottom: 5px;
}

#footer li {
  display: inline;
}

#footer li+li:before {
  content: "-";
  margin-left: 10px;
  margin-right: 10px;
}
